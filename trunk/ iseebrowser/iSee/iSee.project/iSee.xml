<?xml version="1.0"?>
<ISEE>
	<!-- 基本配置信息 -->
	<BASE>
		<HOMEURL VALUE="" /> <!-- 为空字符串时，则显示空白页 -->
		<ENCODING VALUE="gbk" /> <!-- 缺省网页语言编码，如gb2312、gbk、big5 -->
		<CACHESIZE VALUE="16M" /> <!-- 缓冲在内存中的图片占用的内存，单位为兆bytes(M) -->
		<OPENWINDOW VALUE="false" /> <!-- 是否允许打开弹出窗口 -->
	</BASE>
	
	<!-- 应用程序使用的界面文字 -->
	<UI>
		<DIALOG OK="确定" CANCEL="取消" /> <!-- JavaScript对话框按钮标题文字 -->
		<FAVORITE DIALOG="管理收藏夹" DELETE="删除" CLOSE="关闭" TITLE="标题" URL="地址" /> <!-- 收藏夹对话框资源 -->
	</UI>
	
	<!-- 字体文件信息，BOLD/ITALIC的值为：true/false/all，all表示两种字体都支持。
	     INDEX：如果字体文件中包含多个字体，INDEX指明某种字体在文件的索引（从0开始）
	     DEFAULT选取字体列表中的一个，如果FACE没有找到或者没有DEFAULT选项则选择
	     字体列表中的第一个，SIZE缺省为14。
	     FILE=""标明字体文件是和应用程序在同一目录下的sunglobe.ttf
	-->
	<FONT>
		<DEFAULT FACE="Tahoma" SIZE="14" />
		<FACE ALIAS="Tahoma" INDEX="0" BOLD="false" ITALIC="false" FILE="" />
	</FONT>
	
	<!-- 菜单显示资源配置 -->
	<MENU>
		<TOOL VALUE="工具">
			<New VALUE="新窗口" />
			<Close VALUE="关闭窗口" />
			<FullScreen VALUE="全屏模式" />
			<EnableProxy VALUE="启用代理" />
			<Refresh VALUE="刷新" />
			<Stop VALUE="停止" />
			<Back VALUE="后退" />
			<Forward VALUE="前进" />
			<AddFavorite VALUE="加入收藏夹" />
			<ManageFavorites VALUE="管理收藏夹..." />
			<GoFavorite VALUE="打开收藏..." />
			<Options VALUE="选项" />
			<About VALUE="关于..." />
			<Exit VALUE="退出" />
		</TOOL>
		<WINDOW VALUE="窗口" />
	</MENU>
	
	<!-- 代理服务器配置 -->
	<PROXY>
		<ENABLE VALUE="true" />
		<ADDRESS VALUE="10.0.0.172:80" />
		<EXCEPTION VALUE="192.168.*;10.*" />
	</PROXY>
	
	<!-- 载入错误信息 -->
	<ERROR>
		<TITLE VALUE="打开页面失败" />
		<DISPLAY VALUE="载入失败！" />
	</ERROR>
</ISEE>
